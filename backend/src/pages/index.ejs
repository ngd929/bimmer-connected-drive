<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Home</title>
    </head>
    <body>
        <h1>Home Page</h1>

        <main>
            <% vehicle.forEach(data => { %>
                <ul>
                    <li><p><a href="/<%= data._id %>"><%= data.doorLockState %></a></p></li>
                    <li><p><%= data.chargingStatus %></p></li>
                    <li><p><%= data.updateTime %></p></li>
                </ul>
            <% }) %>
        </main>

    </body>
</html>
